	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner myobj = new Scanner(System.in);
		
		
		for(int i=0; i>100;)
		{
			
			System.out.println("enter numb");
			   i += myobj.nextInt();
			System.out.println(i);
		}
	}

}
